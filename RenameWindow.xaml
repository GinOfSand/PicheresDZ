<Window x:Class="Picheres1.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Picheres1"
        mc:Ignorable="d"
        Title="RenameWindow" Height="250" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="25"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="RenameTB" Text="Введите новое название!"></TextBox>
        <Button Content="Принять" Grid.Row="1" Click="Button_Click"/>
        <Button Content="Отмена" Grid.Row="2" Click="Button_Click_1"/>
    </Grid>
</Window>
